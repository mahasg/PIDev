<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Reservation" table="reservation">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idRes" type="integer" column="id_res" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idClient" type="integer" column="id_client" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="matricule" type="string" column="matricule" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="date" column="date" nullable="false"/>
    <field name="duree" type="integer" column="duree" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="frais" type="float" column="frais" precision="10" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
