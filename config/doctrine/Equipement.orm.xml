<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Equipement" table="equipement">
    <indexes>
      <index name="IDX_B8B4C6F32978E8D1" columns="resto_id"/>
      <index name="IDX_B8B4C6F3B4CB84B6" columns="cinema_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ref" type="integer" column="ref" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nome" type="string" column="nome" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prix" type="float" column="prix" precision="10" scale="0" nullable="false"/>
    <field name="dates" type="datetime" column="dates" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <many-to-one field="resto" target-entity="Categorie" fetch="LAZY">
      <join-columns>
        <join-column name="resto_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="cinema" target-entity="Cinema" fetch="LAZY">
      <join-columns>
        <join-column name="cinema_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
