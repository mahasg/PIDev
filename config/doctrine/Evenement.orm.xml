<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Evenement" table="evenement">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idEvent" type="integer" column="id_event" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nomEvent" type="string" column="nom_event" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateEvent" type="date" column="date_event" nullable="false"/>
    <field name="dateEventFin" type="date" column="date_event_fin" nullable="false"/>
    <field name="capacite" type="integer" column="capacite" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="emplacement" type="string" column="emplacement" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
